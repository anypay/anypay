    "anypay-wallet-address-validator": "^0.2.6",
    "asn1.js-rfc5280": "^3.0.0",
    "aws-sdk": "^2.229.1",
    "axios": "^1.3.2",
    "bchaddrjs": "^0.4.3",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^8.0.2",
    "bip70-payment-protocol": "^1.8.0",
    "bitcore-doge-lib": "^8.25.8",
    "bitcore-lib": "^0.16.0",
    "bitcore-lib-cash": "^0.19.0",
    "bluebird": "^3.5.0",
