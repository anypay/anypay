  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/amqplib": "^0.5.17",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-spies": "^1.0.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.17.46",
