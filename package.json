  "scripts": {
    "runtest": "mocha --require=ts-node/register $TEST_PATH --timeout=10000 --exit",
    "console": "ts-node --experimental-repl-await",
    "anypay": "./bin/anypay.ts",
    "start": "ts-node main.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js --watch 'servers/**/*.ts' --exec 'ts-node' servers/rest_api/server.ts",
    "ws:server": "ts-node servers/websocket/server.ts",
    "test": "nyc mocha --require=ts-node/register --timeout=20000 test/integration/*.ts test/types/*_test.ts test/models/*_test.ts test/lib/*_test.ts test/plugins/*_test.ts --exit",
    "docker:build": "rm -rf node_modules && docker-compose build rest-api",
    "build": "tsc",
    "db:migrate": "sequelize db:migrate --url=$DATABASE_URL",
    "db:seed:prices": "ts-node test/seed_prices.ts",
    "payreq:test": "nyc mocha --require=ts-node/register --timeout=20000 --file servers/payment_requests/tests/setup.ts servers/payment_requests/tests/*_test.ts --exit",
    "payreq:test:migrate": "NODE_ENV=test ts-node servers/payment_requests/tests/migrate.ts",
    "payreq:start": "ts-node servers/payment_requests/server.ts"
