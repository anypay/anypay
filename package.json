    "chai-spies": "^1.0.0",
    "codecov": "^3.8.3",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "mocha": "^10.0.0",
    "nodemon": "^1.18.8",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "supertest": "^6.2.2",
    "ts-node": "^6.2.0",
    "typedoc": "^0.23.24"
  }
}
